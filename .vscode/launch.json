{
  // Launch configuration for Flutter web on a fixed port (3000).
  // Select this configuration in the Run and Debug panel and press F5 to start.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter (web:3000)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=3000"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
